{
  "name": "@mcler/cie-color-functions",
  "version": "0.1.2",
  "description": "Human perceived color functions base on LAB/LCH color spaces",
  "keywords": [
    "cie",
    "color",
    "colord",
    "lab",
    "lch",
    "less",
    "lesscss",
    "less.js"
  ],
  "repository": "mcler/cie-color-functions",
  "license": "MIT",
  "author": "Maksim Baranov <mclerishere@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.mjs"
    },
    "./plugins/less": {
      "import": "./plugins/less.mjs",
      "require": "./plugins/less.js",
      "default": "./plugins/less.mjs"
    }
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "index.d.ts",
  "files": [
    "*.{js,mjs,ts,map}",
    "plugins/*.{js,mjs,ts,map}"
  ],
  "scripts": {
    "build": "rm -rf ./dist/* && rollup --config",
    "publish": "npm publish ./dist --access public",
    "release": "npm run build && cp *.json dist && cp *.md dist && npm run publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "colord": "^2.8.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/less": "^3.0.3",
    "@types/node": "^16.10.9",
    "glob": "^7.2.0",
    "jest": "^27.2.5",
    "less": "^4.1.2",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4"
  }
}
